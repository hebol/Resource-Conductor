<!DOCTYPE html>
<html>
  <head>
      <title><%= title %></title>

      <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css">
      <link rel='stylesheet' href='/stylesheets/style.css' />
      <link rel='stylesheet' href='/stylesheets/menu.css' />

      <script src="https://cdn.socket.io/socket.io-1.3.4.js"></script>
      <script type="text/javascript"
              src="//maps.googleapis.com/maps/api/js?key=AIzaSyCyTSwGLixbBgsTj3uJoef-u-qt1Aq9kOU&sensor=false">
      </script>

      <script type="text/javascript" src="/javascripts/jquery.js"></script>
      <script type="text/javascript" src="/javascripts/dateUtil.js"></script>
      <script type="text/javascript" src="/javascripts/serviceDirectory.js"></script>
      <script type="text/javascript" src="/javascripts/map.js"></script>
      <script type="text/javascript" src="/javascripts/time.js"></script>
  </head>
  <body>
    <div id="date">
        <div id="clock"></div>
        <div id="day"></div>
    </div>

    <nav class="menu">
        <input type="checkbox" href="#" class="menu-open" name="menu-open" id="menu-open"/>
        <label class="menu-open-button" for="menu-open">
            <span class="hamburger hamburger-1"></span>
            <span class="hamburger hamburger-2"></span>
            <span class="hamburger hamburger-3"></span>
        </label>

        <a href="#" class="menu-item"> <i class="fa fa-map-marker"></i> </a>
        <a href="#" class="menu-item"> <i class="fa fa-ambulance"></i> </a>
        <a href="#" class="menu-item"> <i class="fa fa-bar-chart"></i> </a>
        <a href="#" onclick="$('#timeControl').toggle()" class="menu-item"> <i class="fa fa-clock-o"></i> </a>
    </nav>

    <div id="timeControl" style="display: none">
        <div class="heading">Time Control</div>
        <div id="currentTime"><span>Current time</span><span id="clock"></span><span id="day"></span></div>
        <div id="setTime"><input type="datetime-local" id="newTime"><button id="setTimeButton">St√§ll in ny tid</button></div>
        <div><span>Speed</span><select id="speedOptions"></select><button id="stopButton">Stop</button><button id="startButton">Start</button></div>
    </div>

    <div id='map-canvas'></div>
  </body>
</html>
